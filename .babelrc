{
  "presets": [
      ["@babel/preset-react",
        {
          //use only necessary pollyfills that are needed in the code
          "useBuiltIns":"usage",
          "target":{
            "browsers" : " 1%,not ie 11 ,not op_mini all",
            "node":"8"
          }
        }
    ],
      "@babel/preset-env",
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/transform-runtime",
    [
      "import",
      {
        "libraryName": "@material-ui/icons",
        "libraryDirectory": "",
        "camel2DashComponentName": false,
      }
  ],
  ]
}
